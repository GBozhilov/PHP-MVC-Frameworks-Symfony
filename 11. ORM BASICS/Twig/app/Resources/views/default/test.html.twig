{% extends 'default/myBase.html.twig' %}

{% block css %}
    <link rel="stylesheet" type="text/css" href="main_special.css">
{% endblock %}

{% block body %}
    <table border="1">
        <tr>
            <th>ID</th>
            <th>FIRST NAME</th>
            <th>LAST NAME</th>
        </tr>

        {% for user in users %}
            <tr>
                <td>{{ loop.index }}</td>
                <td>{{ user.firstName|upper }}</td>
                <td>{{ user.lastName|upper }}</td>
            </tr>
        {% else %}
            Empty list
        {% endfor %}
    </table>
{% endblock %}

{% block footer %}
    <div>Different footer!</div>
{% endblock %}